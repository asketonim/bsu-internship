{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": "error",
        "quotes": ["error", "single"],
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "no-undef": "off",
        "no-console": "off",
        "linebreak-style": "off"
    }
}
